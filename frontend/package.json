{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently 'yarn start:dev' 'yarn tailwind:watch'",
    "start:dev": "ng serve",
    "storybook": "concurrently 'yarn storybook:watch' 'yarn tailwind:watch'",
    "build": "ng build",
    "build:deployment": "ng build --base-href /fleetmanager/",
    "watch": "ng build --watch --configuration development",
    "prettier": "npx prettier --write .",
    "tailwind:watch": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/tailwind-dist.css --watch",
    "test": "ng test",
    "test:e2e": "ng test:e2e",
    "test:cov": "ng test:cov",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.4",
    "@angular/common": "^16.1.4",
    "@angular/compiler": "^16.1.4",
    "@angular/core": "^16.1.4",
    "@angular/forms": "^16.1.4",
    "@angular/platform-browser": "^16.1.4",
    "@angular/platform-browser-dynamic": "^16.1.4",
    "@angular/router": "^16.1.4",
    "@types/echarts": "^4.9.18",
    "ag-grid-angular": "^29.2.0",
    "ag-grid-community": "^29.2.0",
    "echarts": "^5.4.2",
    "fb-watchman": "^2.0.2",
    "luxon": "^3.3.0",
    "ngx-cookie-service": "^16.0.0",
    "ngx-echarts": "^16.0.0",
    "primeicons": "^6.0.1",
    "primeng": "^16.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.4",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "~16.1.4",
    "@angular/compiler-cli": "^16.1.4",
    "@types/jasmine": "~4.3.0",
    "@types/luxon": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ngx-device-detector": "^5.0.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "tailwindcss": "^3.3.2",
    "typescript": "~4.9.4"
  }
}
